var hair_Colo_new = [];
var status = -1;
var beauty = -1;
var hairCard = [2430026];
var types = -1;
var ct = -1;
var facetype = [];
var isSecond = false;
var isAngel;
function start() {
    isAngel = im.getBeginner() == 6001;
    if (isAngel) {
        im.sendChoiceAngle();
        return;
    } else {
        action(1, 0, 0);
    }
}

function action(mode, type, selection) {

    if (mode == 0) {
        im.dispose();
        return;
    } else if (mode == 1) {
        status++;
    } else {
        status--;
    }

    switch (status) {
        case 0:
            if (isAngel) {
                isSecond = selection != 0;
            }
            im.sendSimple("你好。我是大头国的#b#p1012117##k。我可以为你设计一个发型。怎么样？\r\n#b#L0#更换所有【发型】或者【脸型】 (使用神秘盒子)#l \r\n");
            break;
        case 1:
            beauty = selection;
            getHairID(selection);
            var hair = isSecond ? im.getPlayer().getSecondHair() : im.getPlayerStat("HAIR");
            var iNow = 0;
            var hairTypeNow = [];

            hair_Colo_new = im.getCanHair(hair_Colo_new);
            im.sendSimple("那么你想...\r\n#b#L0#自主更换【发型】#l\r\n#b#L4#自主更换【脸型】#l");
            break;
        case 2:
            if (hair_Colo_new.length == 0) {
                im.sendOk("该理发券正在整理中....");
                im.dispose();
            } else if (selection == 0) {
                types = 1;
                //im.askAvatar("那么请随便随便看..", hair_Colo_new, hairCard, isSecond);
				im.sendStyle("我可以改变你的发型,让它比现在看起来漂亮.你为什么不试着改变它下? 我将会帮你改变你的发型,那么选择一个你想要的新发型吧!", hair_Colo_new, hairCard[0],false);

            } else if (selection == 4) {
                beauty = selection;
				types = 100;
				getFaceID(0);
				var iNow = 0;
				var face = isSecond ? im.getPlayer().getSecondFace() : im.getPlayerStat("FACE");
				var faceTypeNow = [];
				for (var i = 0; i < facetype.length; i++) {
                if (facetype[i] != face - face % 1000 + face % 100) {
                    faceTypeNow[iNow++] = facetype[i] + face % 1000 - (face % 100);
					}
				}
				facetype = im.getCanFace(faceTypeNow);
				im.sendStyle("我可以改变你的脸型,让它比现在看起来漂亮. 你为什么不试着改变它下? ,我将会帮你改变你的脸型,那么选择一个你想要的新脸型吧!", facetype, hairCard[0],false);
				types = 100;
				}

            break;
        case 3:
            if (types == 0) {
                status = 0;
                action(mode, type, beauty);
			}else if(types == 100){
				im.setFace(facetype[selection]);
					im.gainItem(2430026,-1);
                    im.sendOk("好了,让朋友们赞叹你的新脸型吧!");
                    im.safeDispose();
            } else {
					im.setHair(hair_Colo_new[selection]);
					im.gainItem(2430026,-1);
                    im.sendOk("好了,让朋友们赞叹你的新发型吧!");
                    im.safeDispose();
            }
            break;
		
        default:
            im.dispose();
            break;
    }
}

